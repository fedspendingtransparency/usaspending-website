    "moment": "2.19.3",
    "moment-timezone": "^0.5.4",
    "mousetrap": "^1.6.1",
    "node-uuid": "^1.4.7",
    "path-to-regexp": "^1.7.0",
    "prop-types": "15.5.10",
    "query-string": "^4.3.4",
    "r-dom": "^2.3.2",
