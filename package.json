    "start": "webpack serve --open --history-api-fallback --progress --config ./webpack/webpack.local.config.js",
    "sass": "webpack serve --open --progress --config ./webpack/webpack.sass.config.js",
    "test": "jest; exit 0;",
    "lint": "eslint --config .eslintrc --ext .jsx,.js \"src/js/**\"; exit 0;",
    "errors": "eslint --config .eslintrc --ext .jsx,.js \"src/js/**\" --quiet; exit 0;",
